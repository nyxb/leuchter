    "@rollup/pluginutils": "^5.0.2",
    "@types/react": "^18.2.14",
    "@vitest/coverage-v8": "^0.32.2",
    "consolji": "^0.1.5",
    "jsonc-parser": "^3.2.0",
    "node-fetch": "^3.3.1",
    "rollup": "^2.79.0",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-typescript2": "^0.34.1",
    "tslib": "^2.6.0",
    "vscode-oniguruma": "^1.7.0",
    "vscode-textmate": "^7.0.0"
  }
}
